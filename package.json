  "scripts": {
    "analyze:client": "npm run clean && npm run build:client --env ANALYZE",
    "clean": "rimraf public",
    "lint": "eslint --quiet --fix src app-dir",
    "build": "babel src --out-dir lib",
    "build:chunks": "npm run clean:chunks && node .dinas/routes-generator/build-chunks.js",
    "build:routes": "webpack --mode production --env routing --config .dinas/routes-generator/routes.webpack.js && node .dinas/routes/buildRoutes",
    "build:client": "webpack --mode production --env client --config .dinas/client-webpack/client.webpack.js --colors",
    "build:dinastico": "webpack --mode production --env static --config .dinas/html-generator/html.webpack.js",
    "test": "npm run lint",
    "cm": "git-cz"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
